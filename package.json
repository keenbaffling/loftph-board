{
  "name": "loftph-board",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:server": "nodemon --inspect ./bin/www",
    "start:client": "npm run --prefix client start",
    "start:concurrently": "concurrently \"npm run start:client\" \"npm run start:server\""
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.1.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "moment": "^2.20.1",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "public/*",
      "client/*"
    ]
  }
}
